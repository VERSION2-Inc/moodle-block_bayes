<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/bayes/db" VERSION="2013060802" COMMENT="XMLDB file for Moodle blocks/bayes"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_bayes_levels" COMMENT="table to store bayes levels" NEXT="block_bayes_likelihoods">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="fullname"/>
        <FIELD NAME="fullname" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="probability"/>
        <FIELD NAME="probability" TYPE="number" LENGTH="10" DECIMALS="5" NOTNULL="true" SEQUENCE="false" PREVIOUS="fullname"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>
    <TABLE NAME="block_bayes_likelihoods" COMMENT="table to store bayes likelihoods" PREVIOUS="block_bayes_levels">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="levelid"/>
        <FIELD NAME="levelid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="questionid"/>
        <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false" PREVIOUS="levelid" NEXT="likelihood"/>
        <FIELD NAME="likelihood" TYPE="number" LENGTH="10" DECIMALS="5" NOTNULL="true" SEQUENCE="false" PREVIOUS="questionid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" />
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
